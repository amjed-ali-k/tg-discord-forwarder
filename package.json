{
  "name": "node-project-template",
  "version": "1.3.0",
  "scripts": {
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "nodemon ./src/index.ts --trace-warnings",
    "start": "ts-node ./src/index.ts --trace-warnings",
    "deploy": "cp -r .deta ./build && cp ./pkg.json ./build && mv ./build/pkg.json ./build/package.json && deta deploy ./build"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.11.9",
    "eslint": "^7.7.0",
    "node-typescript": "^0.1.3",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "strtok3": "^7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16"
  },
  "displayName": "Telegram to Discord",
  "description": "Template for creating Node.js projects including docs and a deploy pipeline",
  "author": "Amjed Ali K",
  "license": "MIT",
  "keywords": [
    "template",
    "skeleton",
    "quickstart"
  ],
  "dependencies": {
    "deta": "^1.1.0",
    "discord.js": "latest",
    "dotenv": "^16.0.3",
    "input": "^1.0.1",
    "telegram": "^2.14.7"
  },
  "source": "src/index.ts",
  "main": "build/index.js",
  "targets": {
    "main": {
      "includeNodeModules": true,
      "context": "node",
      "sourceMap": false,
      "isLibrary": true,
      "optimize": true,
      "outputFormat": "commonjs"
    }
  }
}
